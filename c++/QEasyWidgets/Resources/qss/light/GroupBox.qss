GroupBoxBase {
    color: rgb(45, 33, 21);
	margin-top: 1.5ex;
	background-color: transparent;
	border: 1.2px solid transparent;
	border-radius: 3px;
}

GroupBoxBase::title {
	subcontrol-origin: margin;
	subcontrol-position: top left;
	padding: 3px;
}

GroupBoxBase::indicator:checked {
	border-image: url(:/Icons/Icons/Light/Chevron-Down.svg);
}
GroupBoxBase::indicator:unchecked {
	border-image: url(:/Icons/Icons/Light/Chevron-Up.svg);
}